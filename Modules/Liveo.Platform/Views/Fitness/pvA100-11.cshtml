@{
    ViewBag.Title = "A100-11";
}



<div link="A100-11" class="panel invisible">
    <a   link="A100_11_link" rel="[link='A100-11']" style=""></a>
    @ViewBag.Title_A100_11
    <div class="wrapper-large height-large">
        <div class="title">Add Program</div>
        <div class="col-fifty">
            <label class="large-txt">
                Title:
                <span>
                    <input type="text" />
                </span>
            </label>

            <label class="large-txt">
                Description:
                <span>
                    <textarea></textarea>
                </span>
            </label>
            <label>
                Tags:
                <ul class="tags">
                    <li>fruit</li>
                    <li>vegetable</li>
                </ul>
            </label>
        </div>
        <div class="col-fourty">
            <label class="large-txt">
                Image for this program:
                <span>
                    <input type="file" />
                </span>
                <img src="@Href("../../images/profile-pic.jpg")" height="100px" width="100px" />
            </label>
        </div>
        <div class="clear"></div>
        <div class="row-main-margin-btm">
            <div class="col-hundred">
                <label class="left">Cycles:</label>
                <div class="right">
                    <button link="A100_11_addCycle" class="small-nav-btn-green">Add Cycle</button>
                </div>
            </div>
            <div class="modal-max-large-row scroll-window">
                
                <div class="border-1">
                    <div class="col-fifteen">
                        <img src="@Href("../../images/profile-pic.jpg")" height="100px" width="100px" />
                    </div>
                    <div class="col-sixty">
                        <span class="med-font-bld">Warm up</span><span class="med-font">(Weeks 1-2)</span><br />
                        <hr />
                        Larry North's Ultimate Training - Warm up.
                       
                    </div>
                    <div class="col-twenty txt-right">
                        <a>Edit</a> | <a>Delete</a> | <a>Clone</a>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="border-1">
                    <div class="col-fifteen">
                        <img src="@Href("../../images/profile-pic.jpg")" height="100px" width="100px" />
                    </div>
                    <div class="col-sixty">
                        <span class="med-font-bld">Strength Training</span><span class="med-font">(Weeks 3-4)</span><br />
                        <hr />
                        Larry North's Ultimate Training - Strength Training.
                       
                    </div>
                    <div class="col-twenty txt-right">
                        <a>Edit</a> | <a>Delete</a> | <a>Clone</a>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="border-1">
                    <div class="col-fifteen">
                        <img src="@Href("../../images/profile-pic.jpg")" height="100px" width="100px" />
                    </div>
                    <div class="col-sixty">
                        <span class="med-font-bld">Fat Burning</span><span class="med-font">(Weeks 5-6)</span><br />
                        <hr />
                        Larry North's Ultimate Training - Fat Burning.
                       
                    </div>
                    <div class="col-twenty txt-right">
                        <a>Edit</a> | <a>Delete</a> | <a>Clone</a>
                    </div>
                    <div class="clear"></div>
                </div>
               
            </div>
        </div>
        <div class="right">
            <button link="A100_11_cancel" class="nav-btn-light">CANCEL</button>
            <button link="A100_11_cancel" class="nav-btn-green">SAVE</button>
        </div>
    </div>
</div>

