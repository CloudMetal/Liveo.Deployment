            <span class="header-font-bold">Gary Anderson</span>
            <label>LAST UPDATE: 10/8/2012</label>
            <button link="HP300_healthAssessment" class="small-nav-btn-green right">Health Assessment</button>
        </div>
        <div class="clear"></div>

         <div class="modal-normal-row col-full border-top">
            <div class="col-twenty bg-dark highlight-font-bld modal-normal-row">
                <label>CURRENT CALORIES</label>
               
                <hr>
                 <br />
                <span>
                    <label>Consumed:</label>1525</span><br>
                <br>
                <span>
                    <label>Burned:</label>1800</span>
            </div>
            <div class="col-seventy-five right">
                <div class="col-thirty">
                    <ul>
                        <li><span>Age:</span> 40</li>
                        <li><span>Birth Date:</span> 12/15/1972</li>
                        <li><span>Height:</span> 6'1"</li>
                        <li><span>Weight:</span> 210lbs</li>
                        <li><span>Sex:</span> Male</li>
                    </ul>
                </div>
                <div class="col-thirty">
                    <ul>
                        <li><span>Body Mass Index:</span> 25</li>
                        <li><span>Blood Glucose:</span> 40</li>
                        <li><span>Blood Pressure:</span> 120/80</li>
                        <li><span>Pulse:</span> 80</li>
                        <li><span>BMR:</span> 20.5</li>
                    </ul>
                </div>
                <div class="col-thirty">
                    admin
                    <ul>
                        <li><span>Total Cholesterol:</span> 170</li>
                        <li><span>Hip Circumference:</span> 38</li>
                        <li><span>Waist Circumference:</span> 32</li>
                    </ul>
                </div>
            </div>
        </div>



        <div class="clear"></div>
        <div class="col-full margin-top-ten">
            <button link="HP400_healthInformation" class="segment-small-status-light col-half">Health Information</button>
            <button class="segment-small-status-dark col-half">Daily Program</button>
        </div>
        <div class="clear"></div>


        <div class="col-full">
            <div class="nav-segment-header">
                <span><a id="nav-segment-prev">PREVIOUS DAY</a></span>
                <div class="nav-segmentedcontrol">
                    <button link="HP400_diet" class="right small-nav-btn-light">Diet</button>
                    <button link="HP300-0_exercise" class="right small-nav-btn-dark">Exercise</button>
                </div>
                <span><a id="nav-segment-next">NEXT DAY</a></span>
            </div>
            <div class="clear"></div>


            <div class="col-full margin-top-ten">
                <div class="clear"></div>
                <div class="left">
                    <div class="left">OCTOBER 10, 2012 HEALTH PLAN</div>
                </div>
                <button link="HP400_updateActualExercise" class="right small-nav-btn-green">Add Exercise</button>
                <div class="clear"></div>

            </div>


            <div class="clear"></div>
            <h8 class="margin-top-ten">Actual Exercises Completed</h8>
            <table class="col-full" summary="">
                <caption></caption>
                <thead>
                    <tr>
                        <th>Exercise</th>
                        <th>Total Calories Burned</th>
                        <th>Duration</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="tr1">
                        <td>Warmup</td>
                        <td>100</td>
                        <td>20min</td>
                        <td><a>Remove</a></td>
                    </tr>
                    <tr class="tr2">
                        <td>Strength Training</td>
                        <td>100</td>
                        <td>20min</td>
                        <td><a>Remove</a></td>
                    </tr>
                </tbody>
            </table>
            <label>&nbsp</label>
            <label>&nbsp</label>
            <h8 class="margin-top-ten">Exercise Program Overview</h8>
            <table class="col-full" summary="">
                <caption></caption>
                <thead>
                    <tr>
                        <th>Exercise</th>
                        <th>Total Calories Burned</th>
                        <th>Duration</th>
                        <th width="40px">Actual Duration</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="tr1">
                        <td><a>Warmup</a></td>
                        <td>100</td>
                        <td>45min</td>
                        <td class="small-txt"> <input type="number" /></td>
                        <td> <button class="small-nav-btn-green">Completed</button></td>
                    </tr>
                    <tr class="tr2">
                        <td><a>Strength Training</a></td>
                        <td>100</td>
                        <td>45min</td>
                        <td class="small-txt"> <input type="number" /></td>
                        <td> <button class="small-nav-btn-green">Completed</button></td>
                    </tr>
                </tbody>
            </table>





