@{
    ViewBag.Title = "MediaUpload";
}

<div id="programPopUpmed">
    <div id="album-upload-image">
        <div id="program-form-lft">
            <img src="../Images/profile-pic.jpg" />
        </div>
    </div>
    <div id="program-form-wrap-lft">
        <div id="program-form-lft">
            <span>FILE NAME:</span><br />
            ...file.jpg
        </div>
        <div id="program-form-lft">
            <span>ALBUM TITLE:</span><br />
            UNTITLED ALBUM
        </div>
        <div id="program-form-lft">
            <span>TITLE:</span><br />
            <input type="text" />
        </div>
        <div id="group-form-lft">
            <div class="clear"></div>
            <span>DESCRIPTION:</span><br />
            <textarea></textarea>
        </div>
        <div id="program-form-lft">
            <div class="clear"></div>
        </div>
    </div>
    <br />
    <a id="a1" onclick="cont();" title="Continue" href=""
        class="primaryButtonGreen button" tabindex="0" style="">
        <div class="box">
            <div class="boxContent">
                <div class="padding">SAVE</div>
            </div>
        </div>
    </a>

    <a id="a5" onclick="cont();" title="Continue" href=""
        class="primaryButton button" tabindex="0" style="">
        <div class="box">
            <div class="boxContent">
                <div class="padding">CANCEL</div>
            </div>
        </div>
    </a>

    <div class="clear"></div>
</div>
