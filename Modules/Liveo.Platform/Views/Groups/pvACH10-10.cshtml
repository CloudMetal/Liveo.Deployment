@{
    ViewBag.Title = "ACH10-10";
}

<div link="ACH10-10" class="panel invisible">
    <a link="ACH10-10_link" rel="[link='ACH10-10']" style=""></a>
    @ViewBag.Title_ACH10_10
    <div class="wrapper-large whiteback">
        <div class="title">Add/Edit Challenge</div>
        <div class="col-fifty">
            <div class="col-fourty">
                <label class="medium-txt">
                    Start Date:
                <span>
                    <input type="text" />
                </span>
                </label>
            </div>
            <div class="col-fourty">
                <label class="medium-txt">
                    End Date:
                <span>
                    <input type="text" />
                </span>
                </label>
            </div>
            <div class="clear"></div>
            <label class="large-txt">
                Title:
                <span>
                    <input type="text" />
                </span>
            </label>

            <label class="large-txt">
                Description:
                <span>
                    <textarea>stuff into here</textarea>
                </span>
            </label>


            <label>
                Type:
                <span>
                    <select>
                        <option>Public</option>
                        <option>Private</option>
                    </select>
                </span>
            </label>
        </div>
        <div class="col-fourty">
            <label class="large-txt">
                Location:
                <span>
                    <input type="text" />
                </span>
            </label>
            <label class="large-txt">
                Url:
                <span>
                    <input type="text" />
                </span>
            </label>
            <label class="large-txt">
                Image for this program:
                <span>
                    <input type="file" />
                </span>
                <img src="images/profile-pic.jpg" height="100px" width="100px" />
            </label>
        </div>
        <div class="clear"></div>
        <div class="right">
            <button link="modalcancelbutton" class="nav-btn-light">CANCEL</button>
            <button link="modalokbutton" class="nav-btn-green">SAVE</button>
        </div>
    </div>
</div>


