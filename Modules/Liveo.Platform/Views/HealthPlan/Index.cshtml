}

<script>
    $(document).ready(function () {
    
        CM.hookLink('HT05_editHealthStats', 'popup', 'HT05', 'HP100-10');
        CM.hookLink('HT05_dailyHealthPlan', 'fade', 'HT05', 'HP300');
        CM.hookLink('HP300_healthInformation', 'fade', 'HP300', 'HT05');
        CM.hookLink('HP300_editHealthStats', 'popup', 'HP300', 'HP100-10');
        CM.hookLink('HP400_editHealthStats', 'popup', 'HP400', 'HP100-10');
        CM.hookLink('HP300_updateActualIntake', 'slidenext', 'HP300', 'HP300-10');
        CM.hookLink('HP300_10_cancel', 'slideprev', 'HP300-10', 'HP300');
        CM.hookLink('HP300_10_enterNewMeal', 'popup', 'HP300-10', 'HP300-20');
        CM.hookLink('HP300_exercise', 'fade', 'HP300', 'HP400');
        CM.hookLink('HP400_diet', 'fade', 'HP400', 'HP300');
        CM.hookLink('HP400_updateActualExercise', 'slidenext', 'HP400', 'HP400-10');
        CM.hookLink('HP400_10_cancel', 'slideprev', 'HP400-10', 'HP400');
        CM.hookLink('HP400_10_enterNewExercise', 'popup', 'HP400-10', 'HP400-20');
        CM.hookLink('HT05_viewProgram', 'fade', 'HT05', 'A100-10');
        CM.hookLink('A100_10_cancel', 'fade', 'A100-10', 'HT05');

    });
</script>

<body style="margin: 20px;">
  
    @Html.Partial("pvHT05")
