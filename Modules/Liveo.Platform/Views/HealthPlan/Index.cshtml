    ViewBag.EnableViewTitles = true;
}

@{
    if (ViewBag.EnableViewTitles)
    {
        ViewBag.Title_HT05 = "HT05";
        ViewBag.Title_HP100_10 = "HP100_10";
        ViewBag.Title_HP300 = "HP300";
        ViewBag.Title_HP300_10 = "HP300-10";
        ViewBag.Title_HP300_20 = "HP300-20";
        ViewBag.Title_HP400 = "HP400";
        ViewBag.Title_HP400_10 = "HP400_10";
        ViewBag.Title_HP400_20 = "HP400_20";
        ViewBag.Title_A100_10 = "A100-10";


    }
}


@{
