<div link="UP01" class="panel notinvisible">
    <a link="U0P1_link" rel="[link='U0P1']" style=""></a>
    @ViewBag.Title_UP01
    <h2>User Profile</h2>

    <div id="content" class="clearfix">
        <section id="left">
            <div id="userStats" class="clearfix">
                <div class="pic">
                    <a href="#"><img src="@Href("../../images/g10albumsfunrun3.png")" width="150" height="150" /></a>
                </div>
                <div class="data">
                    <h1>Mary Conroy</h1>
                    <h3>Frisco, TX</h3>
